{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react-jsx",
    "sourceMap": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "incremental": true,
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules", "build"],
  "paths": {
    "@/*": [
      "./*"
    ]
  }
}
